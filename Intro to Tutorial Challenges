import sys

def main():
    NumbersList = []
    NumbersList2 = []
    x1 = sys.stdin.readlines()
    #print x1[0].strip('\n')
    x2 = x1[0].strip('\n')
    #print x2
    x3 = x1[2].strip('\n')
    for i in x3:
        NumbersList.append(i)
    for i in NumbersList:
        if (i != ' '):
            NumbersList2.append(i)
    #print NumbersList2
    for i in range(0, len(NumbersList2)):
        if int(NumbersList2[i]) == int(x2):
            print i

            
main()
